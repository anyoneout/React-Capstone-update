{
    "name": "real-time-highlighter",
    "displayName": "real-time-highlighter",
    "description": "it's a real-time element highlighter",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.96.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand: real-time-highlighter.highlight"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "real-time-highlighter.helloWorld",
                "title": "Start Real-Time-Highlighter"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.10",
        "@types/node": "20.x",
        "@types/vscode": "^1.96.0",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.4.1",
        "eslint": "^9.16.0"
    },
    "dependencies": {
        "ws": "^8.18.0"
    }
}
